<h1>投稿フォーム</h1>
<%= form_with model:@product, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :product_image %>
  <h4>日時</h4>
  <%= f.datetime_select :time %>
  <h4>ショップ名</h4>
  <%= f.text_field :shop_name %>
  <h4>店舗住所</h4>
  <%= f.text_field :address%>
  <h4>料理名</h4>
  <%= f.text_field :menu_neme %>
  <h4>説明</h4>
  <%= f.text_area :description %>
  <%= f.submit '投稿' %>
<% end %>